{
  "settings": {
    "analysis": {
      "filter": {
        "autocomplete_filter": {
          "type": "edge_ngram",
          "min_gram": 3,
          "max_gram": 20,
          "token_chars":[
            "letter",
            "digit"
          ]
        }
      },
      "tokenizer":{
        "edge_ngram_tokenizer":{
          "type":"edgeNGram",
          "min_gram":3,
          "max_gram":20,
          "token_chars":[
            "letter",
            "digit"
          ]
        }
      },
      "analyzer": {
        "autocomplete": {
          "type": "custom",
          "tokenizer": "edge_ngram_tokenizer",
          "filter": [
            "lowercase",
            "asciifolding",
            "autocomplete_filter"
          ]
        }
      }
    }
  },
